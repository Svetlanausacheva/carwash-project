[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    migrations,
    venv,
    env,
    .env,
    .venv,
    *.pyc,
    *.pyo,
    *.pyd,
    .tox,
    .coverage,
    coverage,
    htmlcov,
    build,
    dist,
    *.egg-info,
    .eggs,
    .vscode,
    .idea,
    .pytest_cache,
    .mypy_cache,
    .flake8
per-file-ignores = 
    __init__.py: F401
    tests/*.py: S101
    */migrations/*.py: E501